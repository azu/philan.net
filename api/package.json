{
  "private": true,
  "name": "@philan-net/api",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm-run-all -p dev:next dev:localtunnel",
    "dev:next": "next --port 3000",
    "dev:localtunnel": "lt --port 3000 --subdomain philan-net-api",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "validator-ts": "create-validator-ts \"pages/api/**/api-types.ts\"",
    "test:validator-ts": "create-validator-ts \"pages/api/**/api-types.ts\" --check"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.5",
    "@chakra-ui/react": "^1.3.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "ajv": "^7.1.0",
    "dayjs": "^1.10.4",
    "framer-motion": "^3.3.0",
    "googleapis": "^67.0.0",
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-use": "^17.1.1",
    "simple-oauth2": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.27",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/simple-oauth2": "^4.1.0",
    "create-validator-ts": "^1.0.1",
    "localtunnel": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.1.5"
  },
  "license": "MIT"
